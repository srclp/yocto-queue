{
  "name": "@srclp/yocto-queue",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.10.2",
  "description": "remake yocto-queue",
  "author": "Moozon Wei <muzhenweizh@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/srclp/yocto-queue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srclp/yocto-queue.git"
  },
  "bugs": "https://github.com/srclp/yocto-queue/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish --access=public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.8.10",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "esno": "^0.17.0",
    "lint-staged": "^15.0.2",
    "pnpm": "^8.10.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
